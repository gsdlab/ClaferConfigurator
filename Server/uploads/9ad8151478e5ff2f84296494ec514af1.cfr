abstract Component
    performance : integer	
    energy : integer
	
abstract androidPhone
    hardware : Component ?
        [ this.energy = 10 ]
        [ this.performance = 10 ]
    cpu : Component ?
        [ this.energy = 10 ]
        [ this.performance = 10 ]

    total_performance : integer 
       [ total_performance = sum Component.performance ]
    total_energy : integer
       [ total_energy = sum Component.energy ]  

aPhone : androidPhone

